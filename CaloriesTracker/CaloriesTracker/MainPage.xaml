<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="CaloriesTracker.MainPage"
            xmlns:views="clr-namespace:CaloriesTracker"> <!-- Adjust the namespace as needed -->

    <!-- Page 1 -->
    <NavigationPage Title="Find and Add Products">
        <x:Arguments>
            <views:ProductsPage /> <!-- Use the correct namespace and class name -->
        </x:Arguments>
    </NavigationPage>

    <!-- Page 2 -->
    <NavigationPage Title="Daily Goal Fulfillment">
        <x:Arguments>
            <views:GoalPage /> <!-- Use the correct namespace and class name -->
        </x:Arguments>
    </NavigationPage>

    <!-- Page 3 -->
    <NavigationPage Title="Calories Tracker Stats">
        <x:Arguments>
            <views:StatsPage /> <!-- Use the correct namespace and class name -->
        </x:Arguments>
    </NavigationPage>
</TabbedPage>
